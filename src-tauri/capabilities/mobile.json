{
  "identifier": "mobile-capability",
  "platforms": [
    "android",
    "iOS"
  ],
  "windows": [
    "main"
  ],
  "permissions": [
    "barcode-scanner:default",
    {
        "identifier": "fs:scope",
        "allow": [
            {
                "path": "$APPLOCALDATA/Rauthy.bin"
            }
        ]
    },
    "biometric:default",
    "biometric:allow-biometric-cipher"
  ]
}
